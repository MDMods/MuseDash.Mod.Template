{
  "$schema": "http://json.schemastore.org/template",
  "author": "lxy",
  "classifications": [
    "Muse Dash",
    "Mod",
    "Library"
  ],
  "identity": "MuseDash.Mod.CSharp.Windows",
  "name": "MuseDash Mod",
  "shortName": "musedashmod",
  "tags": {
    "language": "C#",
    "type": "project",
    "platform": "Windows"
  },
  "sourceName": "MuseDashModTemplate",
  "preferNameDirectory": true,
  "symbols": {
    "Setting": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "false",
      "description": "Whether to generate a setting manager class for the mod"
    },
    "Patching": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "true",
      "description": "Whether to generate a patching class for the mod"
    }
  },
  "sources": [
    {
      "exclude": [
        ".idea/**/*",
        ".template.config/**/*",
        "bin/**/*",
        "obj/**/*"
      ],
      "modifiers": [
        {
          "condition": "(!Setting)",
          "exclude": [
            "Data.cs",
            "SettingManager.cs"
          ]
        },
        {
          "condition": "(!Patching)",
          "exclude": [
            "Patches/**/*"
          ]
        }
      ]
    }
  ]
}