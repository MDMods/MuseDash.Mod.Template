{
  "$schema": "http://json.schemastore.org/template",
  "author": "lxy",
  "classifications": [
    "Muse Dash",
    "Mod",
    "Library"
  ],
  "identity": "MuseDash.Mod.CSharp.Windows",
  "name": "MuseDash Mod",
  "shortName": "musedashmod",
  "tags": {
    "language": "C#",
    "type": "project",
    "platform": "Windows"
  },
  "sourceName": "MuseDashModTemplate",
  "preferNameDirectory": true,
  "symbols": {
    "Muse Dash Path": {
      "type": "parameter",
      "description": "Normally ends with steamapps\\common\\Muse Dash",
      "displayName": "Muse Dash Folder Path",
      "datatype": "text",
      "replaces": "steamapps/common/Muse Dash",
      "defaultValue": ""
    }
  },
  "primaryOutputs": [
    {
      "path": "MuseDashModTemplate.csproj"
    },
    {
      "path": "Main.cs"
    },
    {
      "path": "MelonBuildInfo.cs"
    },
    {
      "path": "Setting.cs"
    },
    {
      "path": "AssemblyInfo.cs"
    },
    {
      "path": ".gitignore"
    }
  ],
  "sources": [
    {
      "exclude": [
        ".idea/**/*",
        ".template.config/**/*",
        "bin/**/*",
        "obj/**/*"
      ]
    }
  ]
}